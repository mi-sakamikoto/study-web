<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.login.lg001.mapper.LoginMapper">

    <select id="getAccount" resultType="com.study.login.lg001.dto.AccountDto">
        SELECT ID, PASSWORD, NAME , PERSONID, IDTYPE FROM ACCOUNT where ID = #{id}
    </select>
</mapper>