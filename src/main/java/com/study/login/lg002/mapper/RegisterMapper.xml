<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.login.lg002.mapper.RegisterMapper">

    <insert id="insertAccount" parameterType="com.study.login.lg001.dto.AccountDto">
        INSERT INTO ACCOUNT (ID, NAME, PERSONID, PASSWORD, IDTYPE)
        VALUES (#{id}, #{name}, #{personId}, #{password}, #{idType})
    </insert>
</mapper>
