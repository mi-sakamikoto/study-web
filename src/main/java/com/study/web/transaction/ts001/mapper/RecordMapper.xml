<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.web.transaction.ts001.mapper.RecordMapper">

    <select id="getRecord" resultType="com.study.web.database.entity.assetHistoryEntity">
        SELECT ID, CURRENT_BALANCE, CURRENT_DEPOSIT, CURRENT_WITHDRAW FROM ASSET_HISTORY where ID = #{id}
    </select>
</mapper>